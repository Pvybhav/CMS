{
  "name": "CMS",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "eslint": "^5.1.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "react-scripts": "1.1.4"
  },
  "dependencies": {
    "@material-ui/core": "^1.4.0",
    "@material-ui/icons": "^1.1.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint src/Components",
    "lintFix": "eslint src/Components --fix",
    "eslint": "eslint src/Components --ext .js --ext .jsx --cache || true",
    "eslint:table": "eslint src/Components --ext .js --ext .jsx --cache --format table || true",
    "eslint:summary": "eslint src/Components --ext .js --ext .jsx --cache --format node_modules/eslint-friendly-formatter || true"
  }
